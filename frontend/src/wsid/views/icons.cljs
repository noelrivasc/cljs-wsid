(ns wsid.views.icons
  (:require
   [clojure.string :as str]))

(defn get-icon
  ([icon] (get-icon icon :svg.icon))

  ([icon tag]
   (let [not-svg (not (= ":svg" (first (str/split (str tag) #"\."))))]

     (when not-svg (js/console.error "The tag passed to get-icon must be a keyword starting with :svg"))

     (assoc icon 0 tag))))

